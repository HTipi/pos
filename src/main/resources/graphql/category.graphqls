type Query {
    categories: [Category]
    category(id: ID!): Category
}

type Mutation {
    createCategory(input: CategoryInput): Category
}

 input CategoryInput{
    name: String!,
    nameKh: String!
    enable: Boolean!
}

type Category {
    id: ID!
    name: String!
    nameKh: String!
    enable: Boolean 
}